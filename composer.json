{
    "name": "daedalus/daedalus",
    "homepage": "https://github.com/JoshuaEstes/Daedalus",
    "description": "PHP Task runner",
    "keywords": ["build tool"],
    "license": "MIT",
    "authors": [
        {
            "name": "Joshua Estes",
            "email": "Joshua@Estes.in",
            "homepage": "http://joshuaestes.me"
        }
    ],
    "support": {
        "issues": "https://github.com/JoshuaEstes/Daedalus/issues",
        "wiki": "http://daedalus.readthedocs.org/",
        "source": "https://github.com/JoshuaEstes/Daedalus"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "": "src/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "bin": ["bin/daedalus"],
    "repositories": [
        {
            "name": "sebastian/bytekit-cli",
            "type": "package",
            "package": {
                "name": "sebastian/bytekit-cli",
                "version": "1.0.0",
                "source": {
                    "type": "git",
                    "url": "https://github.com/sebastianbergmann/bytekit-cli.git",
                    "reference": "master"
                }
            }
        }
    ],
    "require": {
        "php": ">=5.4",
        "symfony/class-loader": "~2.3",
        "symfony/config": "~2.3",
        "symfony/console": "~2.3",
        "symfony/dependency-injection": "~2.3",
        "symfony/event-dispatcher": "~2.3",
        "symfony/filesystem": "~2.3",
        "symfony/finder": "~2.3",
        "symfony/process": "~2.3",
        "symfony/yaml": "~2.3"
    },
    "require-dev": {
        "pdepend/pdepend": "*",
        "sebastian/bytekit-cli": "*",
        "sebastian/phpcpd": "*",
        "sebastian/phpdcd": "*",
        "squizlabs/php_codesniffer": "*",
        "phploc/phploc": "*",
        "phpmd/phpmd": "*",
        "mikey179/vfsStream": "*",
        "satooshi/php-coveralls": "*",
        "phpunit/phpunit": "*"
    },
    "suggest": {
        "sebastian/phpcpd": "PHP Copy/Paste Detector",
        "sebastian/phpdcd": "PHP Dead Code Detector",
        "squizlabs/php_codesniffer": "CodeSniffer",
        "phploc/phploc": "PHP Lines of Code",
        "phpmd/phpmd": "PHP Mess Detector",
        "mikey179/vfsStream": "Virtual File System",
        "phpunit/phpunit": "PHPUnit for unit testing"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "archive": {
        "exclude": []
    }
}
