<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="command.chgrp" class="JoshuaEstes\Daedalus\Command\ChgrpCommand">
            <tag name="command" />
        </service>

        <service id="command.chmod" class="JoshuaEstes\Daedalus\Command\ChmodCommand">
            <tag name="command" />
        </service>

        <service id="command.chown" class="JoshuaEstes\Daedalus\Command\ChownCommand">
            <tag name="command" />
        </service>

        <service id="command.copy" class="JoshuaEstes\Daedalus\Command\CopyCommand">
            <tag name="command" />
        </service>

        <service id="command.dump_file" class="JoshuaEstes\Daedalus\Command\DumpFileCommand">
            <tag name="command" />
        </service>

        <service id="command.exec" class="JoshuaEstes\Daedalus\Command\ExecCommand">
            <tag name="command" />
        </service>

        <service id="command.help" class="JoshuaEstes\Daedalus\Command\HelpCommand">
            <argument type="service" id="service_container" />
            <tag name="command" />
        </service>

        <service id="command.lint" class="JoshuaEstes\Daedalus\Command\LintCommand">
            <tag name="command" />
        </service>

        <service id="command.mirror" class="JoshuaEstes\Daedalus\Command\MirrorCommand">
            <tag name="command" />
        </service>

        <service id="command.mkdir" class="JoshuaEstes\Daedalus\Command\MkdirCommand">
            <tag name="command" />
        </service>

        <service id="command.phar" class="JoshuaEstes\Daedalus\Command\PharCommand">
            <tag name="command" />
        </service>

        <service id="command.remove" class="JoshuaEstes\Daedalus\Command\RemoveCommand">
            <tag name="command" />
        </service>

        <service id="command.rename" class="JoshuaEstes\Daedalus\Command\RenameCommand">
            <tag name="command" />
        </service>

        <service id="command.symlink" class="JoshuaEstes\Daedalus\Command\SymlinkCommand">
            <tag name="command" />
        </service>

        <service id="command.touch" class="JoshuaEstes\Daedalus\Command\TouchCommand">
            <tag name="command" />
        </service>
    </services>
</container>
